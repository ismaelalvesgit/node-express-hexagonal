{
  "name": "example",
  "version": "1.0.0",
  "description": "",
  "main": "/src/index.ts",
  "scripts": {
    "dev": "nodemon",
    "build:tsc": "tsc && tscpaths -p tsconfig.json -s . -o ./dist",
    "build:doc": "copyfiles -u 1 doc/**/* dist/doc",
    "build:assets": "copyfiles -u 1 src/public/**/* dist/src",
    "build:views": "copyfiles -u 1 src/views/**/* dist/src",
    "build:i18n": "copyfiles -u 1 src/interface/http/middleware/i18n/locales/**/* dist/src",
    "build": "concurrently \"npm:build:tsc\" \"npm:build:doc\" \"npm:build:assets\" \"npm:build:views\" \"npm:build:i18n\"",
    "test": "cross-env NODE_ENV=test jest --env=node --logHeapUsage",
    "test:unit": "cross-env NODE_ENV=test jest \\.unit\\.test\\.ts --silent",
    "test:integration": "cross-env NODE_ENV=test jest \\.integration\\.test\\.ts",
    "test:coverage": "cross-env NODE_ENV=test jest \\.test\\.ts --coverage",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "setup:up": "npm run migrate:up && npm run seed:run",
    "seed:run": "knex seed:run --env local",
    "migrate:create": "knex migrate:make --env local $MIGRATE_NAME",
    "migrate:down": "knex migrate:down --env local",
    "migrate:up": "knex migrate:latest --env local",
    "conftest": "ts-node-dev conftest.js",
    "cli": "ts-node-dev -r tsconfig-paths/register ./src/command.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ismaelalves/logger": "0.1.3",
    "amqplib": "^0.10.0",
    "axios": "^0.21.4",
    "commander": "^9.4.0",
    "cors": "^2.8.5",
    "cron": "^2.1.0",
    "dotenv": "^8.6.0",
    "ejs": "^3.1.6",
    "elastic-apm-node": "^3.26.0",
    "express": "^4.17.2",
    "express-rate-limit": "^6.6.0",
    "helmet": "^4.6.0",
    "hide-powered-by": "^1.1.0",
    "http-status-codes": "^2.2.0",
    "i18n": "^0.13.4",
    "ioredis": "^4.28.5",
    "joi": "^17.6.0",
    "knex": "^0.95.15",
    "mysql2": "^2.3.3",
    "mysqldump": "^3.2.0",
    "node-cache": "^5.1.2",
    "node-vault": "^0.9.22",
    "nodemailer": "^6.7.2",
    "prom-client": "^13.2.0",
    "ramda": "^0.27.2",
    "response-time": "^2.3.2",
    "socket.io": "^4.5.2",
    "swagger-jsdoc": "^7.0.0-rc.6",
    "swagger-ui-express": "^4.3.0",
    "table": "^6.8.0",
    "uuid": "^8.3.2",
    "x-xss-protection": "^2.0.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.8.2",
    "@types/chance": "^1.1.3",
    "@types/cron": "^2.0.0",
    "@types/i18n": "^0.13.4",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.1.2",
    "@types/knex": "^0.16.1",
    "@typescript-eslint/parser": "^5.36.2",
    "chance": "^1.1.8",
    "concurrently": "^7.4.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.0",
    "eslint-plugin-jest": "^27.0.2",
    "jest": "^29.0.2",
    "nock": "^13.2.9",
    "redis-mock": "^0.56.3",
    "shelljs": "^0.8.5",
    "sqlite3": "^5.1.2",
    "ts-jest": "^29.0.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "tscpaths": "0.0.9",
    "typescript": "^4.8.3"
  }
}
